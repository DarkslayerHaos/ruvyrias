  "description": "A high-performance, rock-solid Lavalink client for Node.js, built for speed and reliability.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc && npm run build:esm",
