  },
  "license": "MIT",
  "keywords": [
    "ruvyrias",
    "lavalink",
    "lavalink-client",
    "discordjs",
    "djs",
    "eris",
    "music-bot",
    "wrapper",
    "library",
    "javascript",
    "typescript",
    "nodejs"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "ws": "^8.18.3"
