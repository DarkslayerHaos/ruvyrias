{
  "name": "ruvyrias",
  "version": "1.3.0",
  "description": "A stable and powerful Lavalink client for NodeJS.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc && npm run build:esm",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:docs": "npx typedoc --plugin typedoc-plugin-markdown --out docs index.ts"
  },
  "homepage": "https://ruvyrias-lock.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarkslayerHaos/ruvyrias.git"
  },
  "author": {
    "name": "DarkslayerHaos",
    "url": "https://github.com/DarkslayerHaos"
  },
  "license": "MIT",
  "keywords": [
    "ruvyrias",
    "lavalink",
    "lavalink-client",
    "discordjs",
    "djs",
    "eris",
    "music-bot",
    "wrapper",
    "library",
    "javascript",
    "typescript",
    "nodejs"
  ],
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "gen-esm-wrapper": "^1.1.3",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.3"
  }
}